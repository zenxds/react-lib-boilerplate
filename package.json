{
  "name": "react-lib-boilerplate",
  "version": "0.1.0",
  "description": "react-lib-boilerplate",
  "main": "dist/main.js",
  "scripts": {
    "start": "BABEL_ENV=development webpack-dev-server -w --open --env=development",
    "build": "BABEL_ENV=production webpack --progress --env=production",
    "prettier": "prettier --write 'lib/**/*.js'",
    "test": "jest ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "yarn test",
      "yarn prettier",
      "git add"
    ]
  },
  "author": {
    "name": "zenxds"
  },
  "license": "MIT",
  "publishConfig": {},
  "repository": {
    "type": "git",
    "url": ""
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/src/"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "antd": "^3.14.1",
    "autoprefixer": "^9.1.5",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.3.1",
    "babel-loader": "^8.0.2",
    "babel-plugin-import": "^1.8.0",
    "css-loader": "^2.1.1",
    "dx-mock": "^0.1.0",
    "eslint": "^5.5.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "image-webpack-loader": "^4.3.1",
    "jest": "^24.3.1",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "lint-staged": "^8.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "moment": "^2.24.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.15.3",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-hot-loader": "^4.8.0",
    "style-loader": "^0.23.0",
    "svg-url-loader": "^2.3.2",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "url-loader": "^1.1.1",
    "webpack": "^4.17.2",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.7"
  },
  "dependencies": {},
  "peerDependencies": {
    "antd": ">=3.0.0",
    "prop-types": ">=15.0.0",
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  }
}
